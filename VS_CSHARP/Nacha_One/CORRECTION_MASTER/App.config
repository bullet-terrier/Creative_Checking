<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <!-- UPDATE_201910 -->
    <!-- 
      Begin integrating some IronPython extensibility functionality.
    -->
    <add key=""/>
    <!-- END UPDATE 201910-->
    <!-- UPDATE_201909 -->
    <!-- ST_SPECIFIC UPDATES MOST ARE DISABLED IN OS VERSION.-->
    <add key="ALLOW_DCT_PROD_QUERY" value="true" />
    <add key="DCT_CONNECTIONSTRING" value="DEBUG" />
    <!-- Lock different types of behavior to certain values.-->
    <add key="DEFAULT_BEHAVIOR" value="LIMITED"/>
    <!--This should match the desired connections string's name.-->
    <add key="DCT_DATE_PARAMETER" value="@drawDate" />
    <!-- your query should contain a "drawdate" variable -->
    <add key="DCT_QUERY_PATH" value="dct_eft_by_batchpayment_number.sql" />
    <!-- These literals will become the primary column names in this new module. -->
    <!-- Make sure these match the output columns of your query when you're updating it.-->
    <!-- If it isn't loading your query - make sure that you have enough headers defined. -->
    <add key="DCT_COLUMN_NAMES" value="Record Number,Tranasction Code,Routing Number,Check Digit,Account Number,Billed Amount,Payment ID,Account Name,Discretionary Data, Addenda Indicator,Trace Placeholder, Duck Creek Account Number" />
    <!--               -->
    <add key="EXPERT_MODE" value="" />
    <!-- If you enable this, we can only assume you know what you're doing.-->
    <add key="PROCESS_ON_OPEN" value="true" />
    <add key="SAVE_ON_CLOSE" value="true" />
    <add key="HANDLE_ADDENDA" value="true" />
    <add key="ALLOW_OLD_EDITOR" value="true" />
    <add key="REMOVE_HEADERS_FOR_CSV" value="true" />
    <!-- UPDATE_20190723 -->
    <add key="AUTOMATICALLY_OPEN_SEARCHED_ENTRY" value="false" />
    <add key="CASE_INSENSITIVE_ENTRY_SEARCH" value="true" />
    <!-- UPDATE_20190725 -->
    <!-- UNCOMMENT THE FOLLOWING LINES TO GAIN CONTROL OF FONTS ON FORMS -->
    <!-- <add key="DEFAULT_APPLICATION_FONT" value="Consolas"/>-->
    <!-- <add key="DEFAULT_APPLICATION_FONTSIZE" value="8"/>-->
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <connectionStrings>
    <add name="DEBUG" connectionString="" />
  </connectionStrings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
